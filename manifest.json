{
"manifest_version": 2,
"name": "Youtube Track",
"description": "Change next and prev track in full disc",
"version": "1.0.3",
"icons": {"16": "img/icon.png",        
		  "19": "img/icon.png",		  
		  "24": "img/icon.png",
		  "32": "img/icon.png",
		  "38": "img/icon.png",
		  "48": "img/icon.png",
		  "128": "img/icon.png"        
},   
	"web_accessible_resources": ["css/estilo.css"],
   "content_scripts": [ {       
      "include_globs": [ "https://www.youtube.com/*" ],
      "js": [ "js/jquery-2.1.4.min.js","js/ver.js"],      
      "matches": ["https://www.youtube.com/*"],    
      "css":["css/estilo.css","css/toastr.min.css"], 
      "run_at": "document_end"      
   } ]
	
}